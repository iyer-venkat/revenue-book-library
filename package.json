{
  "name": "revenue-book-library",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "checks": "tsc && eslint src --ext .js,.ts,.tsx --max-warnings 0 && stylelint --config .stylelintrc.js ./src/**/*.{ts,tsx} --max-warnings 0 && prettier --config .prettierrc.js --ignore-path .prettierignore --check .",
    "dev": "UI_ENV=local vite",
    "fix": "tsc && eslint src --ext .js,.ts,.tsx --max-warnings 0 --fix && stylelint --config .stylelintrc.js ./src/**/*.{ts,tsx} --max-warnings 0 && prettier --config .prettierrc.js --ignore-path .prettierignore --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "UI_ENV=local vite preview",
    "test": "UI_ENV=tests vitest",
    "test:coverage": "UI_ENV=tests vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@snsw/react-component-library": "file:./react-component-library-1.36.1.tgz",
    "core-js": "^3.33.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.17.0",
    "styled-components": "^6.0.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-pathnames": "^1.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^12.10.1",
    "postcss-styled-syntax": "^0.5.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.33.0"
  }
}
